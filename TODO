
# Mozilla
sudo apt-get purge firefox 
sudo apt-get --purge autoremove firefox*
rm -rf ~/.mozilla
rm -rf ~/.mozilla/firefox/

# Libre
sudo apt-get remove --purge libreoffice*
sudo apt-get clean
sudo apt-get autoremove
sudo apt-get remove --purge libreoffice\*
sudo apt-get remove fonts-opensymbol libreoffice libreoffice-\* openoffice.org-dtd-officedocument1.0 python\*-uno uno-libs3-\* ure ure-dbg
sudo apt-get remove libreoffice-core
sudo apt-get remove --purge libreoffice-core
sudo apt-get autoremove -y libreoffice-\*

# Github dektop

wget https://github.com/neel0210/Garbage/releases/download/x/GitHubDesktop-linux-2.9.3-linux.deb
sudo gdebi GitHubDesktop-linux-2.9.3-linux.deb
rm -rf GitHubDesktop-linux-2.9.3-linux.deb

# Github
sudo apt-get install git-all && sudo apt-get install git-lfs

# Brotli
sudo apt install brotli
sudo apt install android-tools-fsutils
sudo apt install simg2img 

